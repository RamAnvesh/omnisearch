<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sudran.appman"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <application
        android:allowBackup="false"
        android:icon="@drawable/omnisearch_main"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <service android:name="com.sudran.omnisearch.android.framework.OmniSearchService"
            android:exported="false"></service>
        <activity
            android:name="com.sudran.omnisearch.android.framework.OmniSearchHome"
            android:label="@string/title_activity_app_man_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>